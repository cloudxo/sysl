    !union NullOrPrivacyPreferences:
        Null
        PrivacyPreferences

    !type Address:
        performedAction <: PerformedAction
        addressUsage <: string:
            @fieldDescription = "Business key for this object"
        addressLineOne <: NullOrString
        addressLineTwo <: NullOrString
        addressLineThree <: NullOrString
        addressLineFour <: NullOrString
        addressLineFive <: NullOrString
        addressLineSix <: NullOrString
        city <: NullOrString
