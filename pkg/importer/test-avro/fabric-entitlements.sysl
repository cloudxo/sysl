        identificationNumber <: NullOrString
        issueLocation <: NullOrString
        expiryDate <: NullOrString
        endDate <: NullOrString
        source <: NullOrString

    !alias Identifiers:
        sequence of Identifier
        
    !type NullOrIdentifiers:
        Null
        Identifiers

    !type PrivacyPreference:
