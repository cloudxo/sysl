        privacyPreferenceType <: string:
            @fieldDescription = "Privacy Preference Type. This along with the category would be the business key for this object"
        privacyPreferenceEntity <: NullOrString
        privacyPreferenceReason <: NullOrString
        privacyPreferenceValue <: NullOrString
        startDate <: NullOrString
        endDate <: NullOrString
        source <: NullOrString

    !alias PrivacyPreferences:
