        contactReference <: NullOrString:
            @fieldDescription = "The actual mobile / phone number or email id"
        startDate <: NullOrString
        endDate <: NullOrString
        source <: NullOrString

    !alias ContactDetails:
        sequence of ContactDetail

    !union NullOrContactDetails:
        Null
        ContactDetail

    !alias Accounts:
        sequence of Account

    !union NullOrAccounts:
        Null
        Accounts

    !type Account:
        performedAction <: PerformedAction
        accountNumber <: string
        productType <: string
        source <: NullOrString
        lastUpdatedDate <: NullOrString
        lastUpdatedBy <: NullOrString
        relationshipTypes <: NullOrPartyAccountRelationships:
            @fieldDescription = "Relationship between party and account e.g., Sole Owner, Joint Owner, etc. A party can have one or more relationships with a given account e.g., Guarantor and Power of Attorney"
        accountKey <: string:
            @fieldDescription = "Account key. System-wide uniqueness is not guaranteed, so this along with source make up the business key for this object"
        companyID <: NullOrString
        branchID <: NullOrString
        accountStatus <: NullOrString
        subProductCode <: NullOrString

    !type PartyAccountRelationship:
        performedAction <: PerformedAction
