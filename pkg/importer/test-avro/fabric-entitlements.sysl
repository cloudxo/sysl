Fabric-entitlements-avro-def "Fabric entitlements avro definitions" [package="com.anz.ocv.pe"]:
    @description =:
        | Fabric entitlements avro definitions.

    #---------------------------------------------------------------------------
    # definitions
    !type OCVEvent:
        header <: Header
        ocvTags <: sequence of string:
            @fieldDescription = "An array of strings that provide further context to the content of this specific message. For example, if the party is ANZx customer, events for that party would have an 'ANZx' tag"
        eventData <: EventData

    !type Header:
        eventUUID <: string:
            @fieldDescription = "A unique identifier for this particular event.  The event producer allocates the eventId. Has to be in UUID v4 format (i.e. 535c3d5d-fc48-4f08-b85c-1a986e9d1234). OCV Event consumers should not use this though, instead they could use correlationId attribute for uniquely identifying an event"
        ocvID <: string:
            @fieldDescription = "A unique OCV identifier for a party. A party can have multiple records in OCV. This and 'subject' would uniquely identify a Party Record within OCV"
        subject  <: Subject:
            @fieldDescription = "The externalPartyIdentifier is an identifier which is NOT a Party Id, but can be used to correlate the Party to a Party Id. For example, a CRN could be used to correlate the customer, or a UUID, or a CAP CIS Id"
        source <: string:
            @fieldDescription = "The service identifier that produced the event"
        occurrenceDateTime <: datetime[~timestamp-millis]:
            @fieldDescription = "The date and time, to millisecond precision, that this event occurred"     
        traceId <: string
        spanId <: string
        parentSpanId <: NullOrString
        correlationId <: int64:
            @fieldDescription = "This would be the Global Sequence Id, unique to each event generated out of MDM"
        hierarchy <: string:
            @fieldDescription = "Business Event Type Hierarchy, i.e. AU/RETAIL/CARDS"
        ignore <: NullOrBool:
            @fieldDescription = "Event should be ignored by the consumers"
        eventType <: string:
            @fieldDescription = "Event Data would be specific for a eventType so this field would help identify what to expect in Event Data"
        expiryDateTime <: NullOrLong:
            @fieldDescription = "The event should be ignored by consumers after the specified expire date and time"
        survivorParty <: SurvivorParty

    !type Subject:
        @recordDescription = "A party can have multiple records in OCV. This and 'ocvID' would uniquely identify a Party Record within OCV"
        subjectType <: string:
            @fieldDescription = "The type of identifier, such as CRN, or OCV Id, or CAPCISId"
        subjectId <: string:
            @fieldDescription = "The actual value of the identifier - that would be the actual CRN, for example"

    !type SurvivorParty:
        @recordDescription = "Survivor Party Details for the Victim party"
        ocvID <: string:
            @fieldDescription = "A unique OCV identifier for the surviving party"
        subject <: Subject

    !type EventData:
        performedAction <: PerformedAction
        lastUpdatedDate <: string:
            @fieldDescription = "The date and time, to millisecond precision, the record was updated last"
        lastUpdatedBy <: string:
            @fieldDescription = "The person / account who last updated the record"
        version <: int32:
            @fieldDescription = "Party level sequence Id. This can help identify missed events, if any"
        createdDate <: NullOrString:
            @fieldDescription = "The date and time, to millisecond precision, the record was first created"
        sinceDate <: NullOrString
        leftDate <: NullOrString
        partyStatus <: NullOrString:
            @fieldDescription = "Indicator whether a Party is Active / Inactive / Deceased / etc"
        partyType <: NullOrOrganisation
        identifiers <: NullOrIdentifiers
        privacyPreferences <: NullOrPrivacyPreferences
        addresses <: NullOrAddresses
        partyEquivalents <: NullOrPartyEquivalents
        contactDetails <: NullOrContactDetails
        accounts <: NullOrAccounts

    !union NullOrOrganisation:
        Null
        Organisation

    !type Organisation:
        organisationType <: NullOrString:
            @fieldDescription = "Type of Organisation"
        industry <: NullOrString:
            @fieldDescription = "Type of Industry the Organisation is in"
        establishedDate <: NullOrString
        kycVerificationLevel <: NullOrString
        kycStatus <: NullOrString
        bankruptDate <: NullOrString
        organisationNames <: NullOrOrganisationNames

    !union NullOrOrganisationNames:
        Null
        OrganisationName

    !alias OrganisationNames:
        sequence of OrganisationName
    
    !type OrganisationName:
        performedAction <: PerformedAction
        nameUsageType <: string:
            @fieldDescription = "This would be the business key for uniquely identifying Organisation Name business object"
        orgName <: NullOrString
        startDate <: NullOrString
        endDate <: NullOrString
        source <: NullOrString

