        occupation <: NullOrString
        kycStatus <: NullOrString
        kycVerificationLevel <: NullOrString
        displayName <: NullOrString
        gender <: NullOrString:
            @fieldDescription="Example: M, F"
        personNames <: NullOrPersonNames
    
    !type PersonName:
        performedAction <: PerformedAction
        nameUsageType <: string:
            @fieldDescription="This would be the business key for uniquely identifying Person Name business object"
        prefix <: NullOrString
        suffix <: NullOrString
